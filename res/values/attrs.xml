<resources>
    <declare-styleable name="ActionBarElement">
        <attr name="align">
            <enum name="middle" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ACtionBarButton">
        <attr name="img" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="LessonView">
        <attr name="titleBG" format="reference|color" />
        <attr name="blkSize" format="integer" />
    </declare-styleable>
</resources>
